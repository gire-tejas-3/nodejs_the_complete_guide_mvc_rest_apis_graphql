							<!-- Basic Bootstrap Table -->
							<div class="card">
								<h5 class="card-header">Leave Details</h5>
							</div>
							<!--/ Basic Bootstrap Table -->
						</div>
						<!-- / Content -->

						<%- include('includes/footer.ejs') %>

						<div class="content-backdrop fade"></div>
					</div>
					<!-- Content wrapper -->
				</div>
				<!-- / Layout page -->
			</div>

			<!-- Overlay -->
			<div class="layout-overlay layout-menu-toggle"></div>
		</div>
		<!-- / Layout wrapper -->

		<%- include('includes/end.ejs') %>
